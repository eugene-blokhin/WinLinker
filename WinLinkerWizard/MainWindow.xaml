<Window x:Class="WinLinker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:WinLinker="clr-namespace:WinLinker"
        Title="Создать ссылку" Height="450" Width="615" ResizeMode="NoResize"
        WinLinker:FrameExtender.FrameExpanse="0,36,0,0" Background="{x:Static SystemColors.ControlBrush}">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="36"/>
            <RowDefinition />
        </Grid.RowDefinitions>        
        <Rectangle Grid.Row="0" MouseDown="HandleClientTitleMouseDown" Fill="Transparent"/>
        <Frame Grid.Row="1" Background="{x:Static SystemColors.ControlBrush}"/>        
    </Grid>
</Window>
